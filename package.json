{
  "name": "guesslot-js",
  "version": "0.0.1",
  "description": "Guesslot JS Library",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "guesslot",
    "defi",
    "ethereum"
  ],
  "scripts": {
    "lint": "prettier src/*.ts src/**/*.ts src/**/*.json --write",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "author": "Guesslot",
